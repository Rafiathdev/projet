    <div class="jp_downlord_main_wrapper">
        <div class=""></div>
        <video id="localVideo" autoplay></video>
        <video id="remoteVideo" autoplay></video>
        <button id="startButton">Démarrer l'appel</button>
        <button id="hangupButton">Raccrocher</button>
        <script>
          const startButton = document.getElementById("startButton");
          const hangupButton = document.getElementById("hangupButton");
          let localStream;
          let remoteStream;
          let pc;
    
          const constraints = { video: true, audio: true };
    
          function gotLocalMediaStream(mediaStream) {
            localStream = mediaStream;
            const localVideo = document.getElementById("localVideo");
            localVideo.srcObject = mediaStream;
          }
    
          function gotRemoteMediaStream(event) {
            remoteStream = event.stream;
            const remoteVideo = document.getElementById("remoteVideo");
            remoteVideo.srcObject = remoteStream;
          }
    
          async function start() {
            startButton.disabled = true;
            hangupButton.disabled = false;
    
            try {
              const stream = await navigator.mediaDevices.getUserMedia(constraints);
              gotLocalMediaStream(stream);
    
              pc = new RTCPeerConnection();
    
              pc.addTrack(stream.getTracks()[0], stream);
              pc.addTrack(stream.getTracks()[1], stream);
    
              pc.ontrack = gotRemoteMediaStream;
    
              const offer = await pc.createOffer();
              await pc.setLocalDescription(new RTCSessionDescription(offer));
    
              // envoie de l'offre aux autres participants
              // par exemple, en utilisant WebSocket
            } catch (err) {
              console.error("Erreur : " + err);
            }
          }
    
          function hangup() {
            pc.close();
            localStream.getTracks().forEach((track) => track.stop());
            remoteStream.getTracks().forEach((track) => track.stop());
            localStream = null;
            remoteStream = null;
            startButton.disabled = false;
            hangupButton.disabled = true;
          }
    
          startButton.onclick = start;
          hangupButton.onclick = hangup;
        </script>
      
    </div>
